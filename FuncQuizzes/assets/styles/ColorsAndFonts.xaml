<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Color -->
    <SolidColorBrush x:Key="Primary-color">#FF9D3C</SolidColorBrush>
    <SolidColorBrush x:Key="Secondary-color">#C5E1F6</SolidColorBrush>
    <SolidColorBrush x:Key="Secondary-background">#D9D9D9</SolidColorBrush>

    <!-- Fonts -->
    <FontFamily x:Key="Kantumruy_Pro">pack://application:,,,/assets/fonts/#Kantumruy Pro</FontFamily>
    <FontFamily x:Key="Suwannaphum">pack://application:,,,/assets/fonts/#Suwannaphum</FontFamily>
    
</ResourceDictionary>